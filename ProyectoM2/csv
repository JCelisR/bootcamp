import csv, json

data = []
with open("contactos.csv", newline='', encoding='utf-8') as f:
    reader = csv.DictReader(f)
    for row in reader:
        row["phone"] = row.get("phone","")
        data.append(row)

with open("contactos.json","w",encoding="utf-8") as f:
    json.dump(data, f, ensure_ascii=False, indent=2)

